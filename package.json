{
  "name": "chatbot-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "node --max-old-space-size=2048 node_modules/.bin/nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node --max-old-space-size=2048 node_modules/.bin/nest start",
    "start:dev": "cross-env NODE_ENV=staging npx nest start --watch",
    "start:dev:staging": "cross-env NODE_ENV=staging npx nest start --watch",
    "start:dev:prod": "cross-env NODE_ENV=production npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node --max-old-space-size=2048 dist/src/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:cli": "npx ts-node cli-tests/test-hybrid-staging.ts",
    "test:cli:hybrid": "npx ts-node cli-tests/test-hybrid-staging.ts",
    "test:cli:open": "npx ts-node cli-tests/test-open-staging.ts",
    "test:cli:closed": "npx ts-node cli-tests/test-closed-staging.ts",
    "test:cli:notification": "node cli-tests/test-notification.js"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.17",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "ai": "^5.0.60",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "helmet": "^8.1.0",
    "jspdf": "^3.0.1",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^22.10.7",
    "@types/papaparse": "^5.3.16",
    "@types/pdfkit": "^0.17.0",
    "@types/supertest": "^6.0.2",
    "axios": "^1.10.0",
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
